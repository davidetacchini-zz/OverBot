{
    "include": [
        "classes",
        "cogs",
        "utils"
    ],
    "exclude": [
        "**/__pycache__",
        "env",
        ".git"
    ],
    "pythonVersion": "3.10",
    "typeCheckingMode": "basic",
    "strictParameterNoneValue": false,
    "reportUnnecessaryTypeIgnoreComment": "warning",
    "reportUnnecessaryCast": "warning",
    "reportGeneralTypeIssues": "none",
    "reportOptionalMemberAccess": "none"
}
